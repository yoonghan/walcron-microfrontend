{
  "name": "walcron-microfrontend",
  "version": "1.0.0",
  "description": "Main start-up for microfrontend",
  "main": "index.js",
  "workspaces": [
    "./profiler",
    "./container",
    "./shared-component"
  ],
  "scripts": {
    "start": "concurrently \"cd profiler; npm run dev\" \"cd container; npm run dev\"",
    "build": "concurrently \"cd profiler; npm run build\" \"cd container; npm run build\"",
    "serve": "concurrently \"cd profiler; npm run start\" \"cd container; npm run start\"",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yoonghan/walcron-microfrontend.git"
  },
  "keywords": [
    "microfrontend"
  ],
  "author": "walcoorperation",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/yoonghan/walcron-microfrontend/issues"
  },
  "homepage": "https://github.com/yoonghan/walcron-microfrontend#readme",
  "dependencies": {
    "concurrently": "^8.1.0"
  },
  "devDependencies": {
    "@types/node": "^20.2.5"
  }
}